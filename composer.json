{
    "name": "rdmumayan/html2pdf-bundle",
    "type": "symfony-bundle",
    "description": "Forked version of Tristan's html2pdf-bundle. Use HTML2PDF as a Symfony2 Service",
    "keywords": ["pdf", "tcpdf", "html2pdf"],
    "homepage": "https://github.com/OwlyCode/EnseparHtml2pdfBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tristan Maindron",
            "email": "tmaindron@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4",
        "symfony/symfony": ">=2.0",
        "rdmumayan/html2pdf": "~0.0"
    },
    "require-dev": {
        "sensio/framework-extra-bundle": "~3.0",
        "phpunit/phpunit": "^7.2 || ^6.5 || ^4.8"
     },
    "autoload": {
        "psr-0": { "Ensepar\\Html2pdfBundle": "" }
    },
    "target-dir": "Ensepar/Html2pdfBundle"
}
